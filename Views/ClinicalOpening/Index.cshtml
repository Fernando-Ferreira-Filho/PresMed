@model PresMed.Models.ClinicOpening

@{
    ViewData["Title"] = $"Horario de atendimento da clinica";

}

@if (TempData["SuccessMessage"] != null) {
    <div class="alert alert-dismissible alert-success">
        <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
        @TempData["SuccessMessage"]
    </div>
}

@if (TempData["ErrorMessage"] != null) {
    <div class="alert alert-dismissible alert-danger">
        <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
        @TempData["ErrorMessage"]
    </div>
}

<div class="row me-5">
    <div class="col-lg-12">
        <span class="display-4">@ViewData["Title"]</span>
    </div>
</div>
<br />

<table class="table table-striped text-center" id="table-contacts">
    <thead>
        <tr>
            <th class="text-center" scope="col">#</th>
           

        </tr>
    </thead>
    <tbody>

        <tr>

            <td>
                <div class="btn-group-sm" role="group">
                    <a role="button" class="btn btn-primary"  asp-controller="Time" asp-action="Edit">Editar</a>
                </div>
            </td>
        </tr>


    </tbody>
</table>